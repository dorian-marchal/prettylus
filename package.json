{
  "name": "prettylus",
  "displayName": "prettylus",
  "description": "Seamlessly format Vue SFCs that use Stylus.",
  "version": "0.1.1",
  "publisher": "ekqnp",
  "license": "MIT",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Formatters",
    "Programming Languages"
  ],
  "keywords": [
    "vue",
    "prettier",
    "stylus",
    "css",
    "sfc",
    "typescript",
    "pug"
  ],
  "homepage": "https://github.com/qnp/prettylus",
	"repository": {
		"type": "git",
		"url": "https://github.com/qnp/prettylus.git"
	},
	"bugs": "https://github.com/qnp/prettylus/issues",
	"icon": "prettylus.png",
	"galleryBanner": {
		"color": "#EB3C3C",
		"theme": "dark"
	},
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "prettylus",
      "properties": {
        "prettylus.stylusSupremacyConfigFileName": {
          "description": "Name of the config file to be used by Stylus Supremacy.",
          "type": "string",
          "default": ".stylusrc"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/prettier": "^2.4.3",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@prettier/plugin-pug": "^1.19.2",
    "prettier": "^2.5.1",
    "stylus-supremacy": "^2.15.0"
  }
}